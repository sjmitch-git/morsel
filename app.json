{
  "expo": {
    "name": "Morsel",
    "slug": "morsel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#111111"
    },
    "assetBundlePatterns": ["**/*"],
    "experiments": {
      "tsconfigPaths": true
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.morsell.morsel"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#111111"
      },
      "package": "com.morsell.morsel"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "219a9f0c-d331-404c-b5f8-3ed4d24b6702"
      }
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location.",
          "locationWhenInUsePermission": "Show current location on map."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera.",
          "version": "latest"
        }
      ],
      [
        "expo-av",
        {
          "permissions": ["audioRecording"]
        }
      ]
    ],
    "owner": "sjmitch",
    "theme": {
      "dark": true,
      "primary": "indigo",
      "secondary": "blue",
      "active": "white",
      "inactive": "midGrey"
    }
  }
}
